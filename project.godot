2d_physics/layer_5="Botão"
2d_physics/layer_8="Porta"
2d_physics/layer_9="Morte"
2d_physics/layer_16="Saída"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
